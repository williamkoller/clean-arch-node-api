{
  "version": 2,
  "name": "clean-arch-node-api",
  "public": true,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["dist/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "@app/(.*)",
      "dest": "dist/main.js"
    }
  ]
}
